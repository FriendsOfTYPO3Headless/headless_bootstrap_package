lib.contentElementWithHeader {
  fields {
    content {
      fields {
        date = TEXT
        date {
          field = date
          strftime = {$dateformat}
          if {
            isGreaterThan.field = date
            value = 0
          }
        }

        _localizedUid = TEXT
        _localizedUid {
          field = _LOCALIZED_UID
        }
      }
    }
  }
}